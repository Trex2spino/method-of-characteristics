{
    "global plot settings":{
        "theme": "light",
        "figure size": [14,9],
        "xlim": [0, 5],
        "ylim": [0, 1.2]
    },
    "CHARACTERISTIC MESH":{
        "type": "mesh",
        "annotate": false,
        "show mass flow": true
    },
    "MACH NUMBER":{
        "type": "scalar",
        "parameter": "mach",
        "show surface plots": true,
        "colorbar label": "Mach"
    },
    "PRESSURE RATIO":{
        "type": "scalar",
        "parameter": "pressure ratio",
        "show surface plots": true, 
        "colorbar label": "$p/p_0$"
    },
    "TEMPERATURE RATIO":{
        "type": "scalar",
        "parameter": "temperature ratio",
        "show surface plots": false, 
        "colorbar label": "$T/T_0$"
    },
    "DENSITY RATIO":{
        "type": "scalar",
        "parameter": "density ratio",
        "show surface plots": true, 
        "colorbar label": "$\\rho / \\rho_0$"
    },
    "VELOCITY": {
        "type": "scalar",
        "parameter": "velocity",
        "show surface plots": true
    }

}